.cateListContainer swiper {
  width: 100%;
  height: 370rpx;
}
.cateListContainer swiper image {
  width: 100%;
  height: 370rpx;
}
.cateListContainer .listContainer .headTitle .PrimaryTitle {
  font-size: 35rpx;
  font-weight: bold;
  text-align: center;
  padding: 15rpx 0;
}
.cateListContainer .listContainer .headTitle .secondaryTitle {
  font-size: 25rpx;
  text-align: center;
  padding-bottom: 9rpx;
}
.cateListContainer .listContainer .ListGoodGrid {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  -webkit-justify-content: space-around;
          justify-content: space-around;
}
.cateListContainer .listContainer .ListGoodGrid:after {
  content: "";
  width: 45%;
  height: 0;
}
.cateListContainer .listContainer .ListGoodGrid .listItem {
  width: 45%;
  margin-bottom: 9rpx;
}
.cateListContainer .listContainer .ListGoodGrid .listItem .listItemImg {
  width: 100%;
  height: 345rpx;
}
.cateListContainer .listContainer .ListGoodGrid .listItem .listItemName {
  font-size: 27rpx;
}
.cateListContainer .listContainer .ListGoodGrid .listItem .listItemNPrice {
  color: #f00;
  font-weight: bold;
  font-size: 27rpx;
}

